function [x,k] = MG(A,b)

N = size(A,1);
m = 4;

% Make smoother
D = diag(diag(A));
[L,U] = lu(A);
D = diag(A);
L = L - D;
Mrbgs = L*inv(D)*U;
Mjac = D;

[L,Plist,Nlist] = setup(N);


resid_norm0 = sqrt(b'*b);
resid_norm = resid_norm0;
x = zeros(Nlist(L),1);
k = 0;
while resid_norm/resid_norm0 > 1e-4 && k<1000
    x = MV(A,b,Mjac,m,x,Plist,Nlist,L,L);
    resid_norm = sqrt((A*x-b)'*(A*x-b));
    k = k + 1;
end

end